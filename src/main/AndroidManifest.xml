<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.example.zdemoappo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZDemoAppo">
        <activity android:name=".sellerPart.AddProduct"></activity>
        <activity android:name=".sellerPart.SSignUpActivity" />
        <activity android:name=".sellerPart.SLoginActivity" />
        <activity android:name=".userPart.ProductInfoActivity" />
        <activity android:name=".userPart.MSignUpActivity" />
        <activity android:name=".userPart.HomeActivity" />
        <activity android:name=".userPart.MLoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>